#!/bin/bash
# Voir cette page : https://misc.flogisoft.com/bash/tip_colors_and_formatting

export NOIR=$'\e[0;30m'
export ROUGE=$'\e[0;31m'
export VERT=$'\e[0;32m'
export JAUNE=$'\e[0;33m'
export BLEU=$'\e[0;34m'
export MAGENTA=$'\e[0;35m'
export CYAN=$'\e[0;36m'
export BLANC=$'\e[0;37m'

export NOIR2=$'\e[1;30m'
export ROUGE2=$'\e[1;31m'
export VERT2=$'\e[1;32m'
export JAUNE2=$'\e[1;33m'
export BLEU2=$'\e[1;34m'
export MAGENTA2=$'\e[1;35m'
export CYAN2=$'\e[1;36m'
export BLANC2=$'\e[1;37m'

export GRIS=$'\e[0;38;5;238m'
export GRIS2=$'\e[0;38;5;245m'

export ORANGE=$'\e[0;38;5;214m'

export STANDARD=$'\e[1;0m'
export NEUTRE=$'\e[0m'

export GRAS=$'\e[1m'
export NONGRAS=$'\e[21m'
export SURLIGNER=$'\e[4m'
export NONSURLIGNER=$'\e[24m'
export CLIGNOTANT=$'\e[5m'
export NONCLIGNOTANT=$'\e[25m'
export SOULIGNER=$'\e[7m'
export NONSOULIGNER=$'\e[27m'
export BARRE=$'\e[9m'
export NONBARRE=$'\e[29m'

export TEST=$"\033[35m"
export TEST2=$"\e[38;5;82m"