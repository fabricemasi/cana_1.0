#!/bin/bash

# INIT 01 : Creation de la variable VERBOSE
#           Creation de la variable AUTORUN


init() {
    if [[ $1 == "" ]];then
        export VERBOSE=0
    fi
    if [[ $# == 1 ]];then
        export VERBOSE=$1
    fi

    source "$BIN/init_02"   # ------------------------------------------------> init 02
}


if [[ $(cat "$BIN/data/autorun.sh") != "" ]]; then
    export AUTORUN=1
else
    export AUTORUN=0
fi


